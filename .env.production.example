# Production Environment Variables
NODE_ENV=production
PORT=5000

# URLs
API_URL=https://api.eliteeventskenya.com
FRONTEND_URL=https://eliteeventskenya.com

# Database
DATABASE_URL=postgresql://user:password@host:5432/elite_events_kenya

# Redis
REDIS_URL=redis://host:6379

# JWT
JWT_SECRET=your_production_jwt_secret_here
JWT_REFRESH_SECRET=your_production_refresh_secret_here
JWT_EXPIRES_IN=7d
JWT_REFRESH_EXPIRES_IN=30d

# AWS S3
AWS_ACCESS_KEY_ID=your_production_key
AWS_SECRET_ACCESS_KEY=your_production_secret
AWS_REGION=us-east-1
AWS_BUCKET_NAME=elite-events-production

# SendGrid
SENDGRID_API_KEY=SG.your_production_key
FROM_EMAIL=noreply@eliteeventskenya.com

# Africa's Talking
AT_API_KEY=your_production_key
AT_USERNAME=your_username
AT_SENDER_ID=ELITEEVENTS

# M-Pesa (Production)
MPESA_CONSUMER_KEY=your_production_key
MPESA_CONSUMER_SECRET=your_production_secret
MPESA_SHORTCODE=your_shortcode
MPESA_PASSKEY=your_production_passkey
MPESA_CALLBACK_URL=https://api.eliteeventskenya.com/api/payments/mpesa/callback

# Stripe (Production)
STRIPE_SECRET_KEY=sk_live_your_production_key
STRIPE_WEBHOOK_SECRET=whsec_your_production_secret

# PayPal (Production)
PAYPAL_CLIENT_ID=your_production_id
PAYPAL_CLIENT_SECRET=your_production_secret
PAYPAL_MODE=live

# CORS
CORS_ORIGIN=https://eliteeventskenya.com

# Logging
LOG_LEVEL=info
